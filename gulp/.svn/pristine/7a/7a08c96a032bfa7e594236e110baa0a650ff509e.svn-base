/* 
	common
 */
 @import "../tobe/function";
 @import "compass";
 @import "../tobe/components/components-base";
 /* touchslide */
 #touchSlide{
 	position: relative;
 	width: 100%;
 	overflow: hidden;
 	height: rem(240);
 }
 .swiper-slide a{
 	display: block;
 }
 .swiper-wrapper{
 	.swiper-slide{
 		position: relative;
 	}
 }
 .swiper-pagination{
 	position: absolute;
 	bottom: rem(16);
 	left: 50%;
 	@include transform(translate3d(-50%, 0, 0));
 	li {
 	    display: inline-block;
 	    width: rem(16);
 	    height: rem(16);
 	    @include border-radius(20px);
 	    text-indent: -9999px;
 	    overflow: hidden;
 	    margin: 0 rem(12);
 	    border: 1px solid #fff;
 	    background-color: transparent;
 	    &.on{
 	    	background:#fff;
 	    }
 	}

 }
 
 .floor-container{
 	width: 100%;
    overflow: hidden;
    font-size: 0;
    margin-bottom: rem(20);
    background-color: #fff;
    .grid{
    	position: relative;
    	@include hairline(bottom, #e4e4e4);
    }
 }

 .shop-listText{
 	color: $grayDarker;
 	font-size: rem(28);
 	line-height: 1.2;
 	height: .66rem;
 	@include clamp();
 }
 .shop-price{
 	display: table-cell;
 	font-size: rem(28);
 	vertical-align: middle;
 	padding: rem(8) 0;
 	font-family: helvetica;
 }

  /* loading */
 .bl-ctn-loading{
 	@include transition(opacity .5s);
     position: fixed;
     left: 0;
     top: 0;
     width: 100%;
     height: 100%;
     z-index: 99999;
     background: #eee;
     text-align: center;
     font-size: .28rem;
     &.hide{
     	opacity: 0;
     }
     .prompt-loading{
     	position: absolute;
 	    top: 60%;
 	    left: 50%;
 	    @include transform(translate(-50%, -50%));
 	    overflow: visible;
 	    color: #fff;
 	    .preloader{
 	    	margin: 0 auto;
 	    }
     }
 }

/* header bar */
.logo-button{
	width: .9rem;
	height: .6126rem;
	background-image: url('data:image/png;base64,');
	background-position: center center;
	background-repeat: no-repeat;
	background-size: contain;
	margin-left: .05rem;
}
.bar--padding .button{
	margin: 0 .1rem;
}
.bar .title.index-title{
	left: 20%;
	right: 20%;
}
.title-input{
	position: relative;
	width: 100%;
	height: .65rem;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: .3rem auto;
	background-position: left center;
	font-size: 0;
	.input-control{
		width: 100%;
		height: .65rem;
		line-height: .65rem;
		padding: 0;
		padding-left: rem(40);
		text-align: left;
		font-family: "Helvetica";
		font-size: rem(28);
		background-color: transparent;
		outline: 0;
		border: 0;
		color: #999;
		@include box-sizing(border-box);
		&::-webkit-input-placeholder{
			color: #999;
		}
	}
	@include hairline(bottom, #ddd);
	@include box-sizing(border-box);
}

/* adv2 */
.room-2{
	margin: rem(20) 0;
	padding: 0 rem(10);
	background-color: #fff;
	.cell a{
		display: block;
		padding: 0 rem(10);
	}
}

.recommend-title{
	padding: rem(12) rem(20);
	@include display(flex);
	@include justify-content(space-between);
	font-size: rem(24);
	@include hairline(bottom, #e4e4e4);
	position: relative;
	&.room-flash{ padding-left: 0;}
	a{
		color: $grayDarker;
	}
	h4{
		@include display(flex);
		@include justify-content(center);
		@include align-items(center);
		.lef-red{
			width: 2px;
			height: rem(24);
			background-color: $red;
			margin-right: rem(10);
		}
		font-size: rem(24);
		color: $grayDarker;
		font-weight: normal;
	}
}
.shop-blod{
	.cell{
		padding: 0 rem(20);
	}
}

.form-control{
	width: 80%;
	height: rem(68);
	margin-left: 14px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #ddd;
	background-image: url('data:image/png;base64,');
	background-position: 5px center;
	background-repeat: no-repeat;
	background-size: 18px auto;
	.input{
		height: 100%;
		-webkit-border-radius: 5px;
		border-radius: 5px;
		font-size: rem(28);
		background-color: transparent;
		padding-left: 30px;
		line-height: normal;
		&::-webkit-input-placeholder{
			line-height: normal;
		}
	}
}
.half-width{
	width: rem(20);
}
.search-list .grid{
	background-color: #f4f4f4;
	height: rem(90);
	border-bottom: 1px solid #ddd;
}
.cancle-search{
	padding-left: rem(32);
	color: #a1a1a1;
	font-size: rem(28);
	&:active{
		opacity: .6;
	}
}

.search-title{
	font-size: rem(28);
	color: #808281;
	padding: rem(10) rem(30);
}

.footer {
	.sublogin{
		background-image: inline-image("../images/base64/user.png");
		background-repeat: no-repeat;
		background-size: rem(42);
		background-position: rem(10) center;
		padding-left: rem(60);
		line-height: rem(40);
		a{
			font-size: rem(24);
			color: #fe5d4c;
			padding: 0 rem(12);
			display: inline-block;
			line-height: rem(40);
		}
	}
	.platforms{
		margin-top: rem(10);
		background-color: #fff;
		line-height: rem(40);
		padding: rem(10) 0 rem(30);
		a{
			color: #333;
		}
	}
}
.copyright{
	text-align: center;
	color: #999;
	line-height: rem(40);
	margin-top: rem(10);
}

#app-download{
	width: 100%;
    height: auto;
    max-width: 750px;
    min-width: 320px;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 9999;
    .close{
		position: absolute;
	    left: 0;
	    top: 25%;
	    width: 10%;
	    height: 50%;
    }
}







