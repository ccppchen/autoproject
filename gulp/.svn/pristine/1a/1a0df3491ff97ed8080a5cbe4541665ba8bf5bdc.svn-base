@font-face {
	font-family: "myfont";
	src: url("data:application/x-font-ttf;data:;base64,") format('truetype');
}

.iconfont:before {
	font-family: "myfont";
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
	font-style: normal;
	font-variant: normal;
	font-weight: normal;
	/* speak: none; only necessary if not using the private unicode range (firstGlyph option) */
	text-decoration: none;
	text-transform: none;  
}


.iconfont-class:before {
	content: "\E001";
}

.iconfont-shop-car:before {
	content: "\E002";
}
